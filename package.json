{
    "name": "Slyde",
    "version": "1.0.0",
    "description": "",
    "main": "./src/index.js",
    "jest": {
        "setupFilesAfterEnv": [
            "<rootDir>/src/misc/setupTests.js"
        ],
        "testPathIgnorePatterns": [
            "/node_modules/"
        ],
        "moduleNameMapper": {
            "\\.(css|less|scss|sss|styl)$": "<rootDir>/node_modules/jest-css-modules"
        },
        "moduleFileExtensions": [
            "js",
            "jsx",
            "json",
            "ts",
            "tsx"
        ]
    },
    "scripts": {
        "test": "jest --watchAll",
        "test-once": "jest",
        "build": "webpack --mode production",
        "start": "webpack-dev-server --config ./webpack.config.js --mode development --hot",
        "storybook": "start-storybook -p 6006",
        "build-storybook": "build-storybook"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        ".{js,jsx}": [
            "prettier --write",
            "eslint --fix",
            "jest"
        ],
        "*.{css,md}": [
            "echo 'Checking files with Eslint/Prettier and running unit tests...'",
            "prettier --write",
            "eslint --fix",
            "git add"
        ]
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@babel/core": "^7.2.2",
        "@babel/plugin-proposal-class-properties": "^7.4.4",
        "@babel/polyfill": "^7.2.5",
        "@babel/preset-env": "^7.3.1",
        "@babel/preset-react": "^7.0.0",
        "@storybook/addon-actions": "^5.0.10",
        "@storybook/addon-info": "^5.0.10",
        "@storybook/addon-links": "^5.0.10",
        "@storybook/addons": "^5.0.10",
        "@storybook/react": "^5.0.10",
        "babel-eslint": "^10.0.1",
        "babel-loader": "^8.0.5",
        "css-loader": "^2.1.1",
        "eslint": "^5.16.0",
        "eslint-config-airbnb": "^17.1.0",
        "eslint-config-prettier": "^4.0.0",
        "eslint-loader": "^2.1.2",
        "eslint-plugin-import": "^2.14.0",
        "eslint-plugin-jsx-a11y": "^6.1.1",
        "eslint-plugin-prettier": "^3.0.1",
        "eslint-plugin-react": "^7.11.0",
        "expect": "^24.7.1",
        "husky": "^1.3.1",
        "jest": "^24.7.1",
        "jest-css-modules": "^2.0.0",
        "lint-staged": "^8.1.3",
        "prettier": "^1.16.4",
        "prettier-webpack-plugin": "^1.2.0",
        "react-test-renderer": "^16.8.1",
        "storybook-addon-specifications": "^2.1.2",
        "style-loader": "^0.23.1",
        "webpack": "^4.29.3",
        "webpack-cli": "^3.2.3",
        "webpack-dev-server": "^3.1.14"
    },
    "dependencies": {
        "@fortawesome/fontawesome-svg-core": "^1.2.17",
        "@fortawesome/free-solid-svg-icons": "^5.8.1",
